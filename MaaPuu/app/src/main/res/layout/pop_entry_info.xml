<?xml version="1.0" encoding="utf-8"?>
<com.flyco.roundview.RoundRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="270dp"
    android:layout_height="450dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    app:rv_backgroundColor="@color/white"
    app:rv_cornerRadius="5dp"
    android:gravity="center">
    <com.flyco.roundview.RoundLinearLayout
        android:layout_width="270dp"
        android:layout_height="450dp"
        android:orientation="vertical"
        app:rv_backgroundColor="@color/white"
        app:rv_cornerRadius="5dp"
        android:gravity="center">
        <com.maapuu.mereca.view.CircleImgView
            android:id="@+id/iv_image"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:src="@mipmap/morentouxiang"/>
        <TextView
            android:id="@+id/txt_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="你离时尚只差一个范"
            android:textSize="16sp"
            android:textColor="@color/text_33"/>
        <TextView
            android:id="@+id/txt_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="15835249625"
            android:textSize="14sp"
            android:textColor="@color/text_99"/>
        <com.flyco.roundview.RoundTextView
            android:id="@+id/txt_1"
            android:layout_width="195dp"
            android:layout_height="45dp"
            android:gravity="center"
            android:layout_marginTop="30dp"
            android:text="录入会籍"
            android:textColor="@color/text_33"
            app:rv_cornerRadius="23dp"
            app:rv_backgroundColor="@color/white"
            app:rv_strokeWidth="1dp"
            app:rv_strokeColor="@color/text_33"/>
        <com.flyco.roundview.RoundTextView
            android:id="@+id/txt_2"
            android:layout_width="195dp"
            android:layout_height="45dp"
            android:gravity="center"
            android:layout_marginTop="10dp"
            android:text="录入项目卡"
            android:textColor="@color/text_33"
            app:rv_cornerRadius="23dp"
            app:rv_backgroundColor="@color/white"
            app:rv_strokeWidth="1dp"
            app:rv_strokeColor="@color/text_33"/>
        <com.flyco.roundview.RoundTextView
            android:id="@+id/txt_3"
            android:layout_width="195dp"
            android:layout_height="45dp"
            android:gravity="center"
            android:layout_marginTop="10dp"
            android:text="录入充值卡"
            android:textColor="@color/text_33"
            app:rv_cornerRadius="23dp"
            app:rv_backgroundColor="@color/white"
            app:rv_strokeWidth="1dp"
            app:rv_strokeColor="@color/text_33"/>
        <com.flyco.roundview.RoundTextView
            android:id="@+id/txt_4"
            android:layout_width="195dp"
            android:layout_height="45dp"
            android:gravity="center"
            android:layout_marginTop="10dp"
            android:text="录入套餐"
            android:textColor="@color/text_33"
            app:rv_cornerRadius="23dp"
            app:rv_backgroundColor="@color/white"
            app:rv_strokeWidth="1dp"
            app:rv_strokeColor="@color/text_33"/>
    </com.flyco.roundview.RoundLinearLayout>
    <ImageView
        android:id="@+id/iv_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/guanbi"
        android:padding="5dp"
        android:layout_alignParentRight="true"/>
</com.flyco.roundview.RoundRelativeLayout>
